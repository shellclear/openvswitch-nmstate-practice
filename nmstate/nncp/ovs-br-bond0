# Essa bridge Ã© criada sobre o bond0
apiVersion: nmstate.io/v1
kind: NodeNetworkConfigurationPolicy
metadata:
  name: ovs-br-bond0
spec:
  nodeSelector:
    node-role.kubernetes.io/worker: ""
  desiredState:
    interfaces:
    - name: br-bond0
      description: ovs bridge with bond0 as a port
      type: ovs-bridge
      state: up
      bridge:
        options:
          stp: false
        port:
        - name: bond0